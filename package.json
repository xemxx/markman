{
  "name": "markman",
  "version": "0.2.4",
  "main": "dist/electron/main/index.js",
  "description": "Really simple Electron + Vue + Vite boilerplate.",
  "displayName": "Markman",
  "license": "GPL-3.0-or-later",
  "repository": "https://github.com/xemxx/markman-client",
  "author": "xem <xemxx@qq.com>",
  "private": true,
  "scripts": {
    "dev": "vite",
    "vue:tools": "vue-devtools",
    "build": "vue-tsc --noEmit && vite build && electron-builder",
    "build:all": "vue-tsc --noEmit && vite build && electron-builder -mwl",
    "lint": "eslint src electron  --ext .ts,.tsx,.vue",
    "lint:fix": "eslint src electron --fix --ext .ts,.tsx,.vue",
    "release": "vue-tsc --noEmit && vite build && electron-builder --publish onTagOrDraft",
    "patch": "npm version patch && git push origin master && git push origin --tags",
    "minor": "npm version minor && git push origin master && git push origin --tags",
    "major": "npm version major && git push origin master && git push origin --tags"
  },
  "engines": {
    "node": ">=14.17.0"
  },
  "dependencies": {
    "@electron/remote": "^2.0.8",
    "electron-about": "^2.0.1",
    "electron-devtools-installer": "^3.1.1",
    "electron-log": "^4.2.2",
    "electron-store": "^8.0.0",
    "regenerator-runtime": "^0.13.7",
    "sqlite3": "^5.0.0"
  },
  "devDependencies": {
    "@ant-design/icons-vue": "^6.1.0",
    "@types/sqlite3": "^3.1.8",
    "@typescript-eslint/eslint-plugin": "^5.30.6",
    "@typescript-eslint/parser": "^5.30.6",
    "@vitejs/plugin-vue": "^2.0.0",
    "@vue/devtools": "^6.2.1",
    "ant-design-vue": "^3.0.0",
    "axios": ">=0.21.1",
    "core-js": "^3.6.5",
    "electron": "^25.3.0",
    "electron-builder": "^23.1.0",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.2.0",
    "highlight.js": ">=10.4.1",
    "less": "^4.1.3",
    "less-loader": "^11.0.0",
    "marked": "^4.0.10",
    "mitt": "^3.0.0",
    "perfect-scrollbar": "^1.5.0",
    "pinia": "^2.0.16",
    "prettier": "^2.7.1",
    "stylus": "^0",
    "stylus-loader": "^7.0.0",
    "typescript": "^4.7.4",
    "unplugin-vue-components": "^0.25.1",
    "uuid": "^8.0.0",
    "vditor": "^3.0.0",
    "vite": "^2.0.0",
    "vite-plugin-electron": "^0.8.0",
    "vite-plugin-svg-icons": "^2.0.1",
    "vue": "^3.0.0",
    "vue-router": "^4.0.0",
    "vue-tsc": "^0.38.3"
  },
  "env": {
    "VITE_DEV_SERVER_HOST": "127.0.0.1",
    "VITE_DEV_SERVER_PORT": 3344
  },
  "keywords": [
    "electron",
    "rollup",
    "markdown",
    "editor",
    "vite",
    "vue3",
    "vue"
  ]
}
