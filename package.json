{
  "name": "markman",
  "version": "0.3.0",
  "main": "dist/electron/main/index.js",
  "description": "Really simple Electron + Vue + Vite boilerplate.",
  "displayName": "Markman",
  "license": "GPL-3.0-or-later",
  "repository": "https://github.com/xemxx/markman-client",
  "author": "xem <xemxx@qq.com>",
  "private": true,
  "scripts": {
    "dev": "vite",
    "vue:tools": "vue-devtools",
    "build": "vue-tsc --noEmit && vite build && electron-builder",
    "build:all": "vue-tsc --noEmit && vite build && electron-builder -mwl",
    "lint": "eslint src electron  --ext .ts,.tsx,.vue",
    "lint:fix": "eslint src electron --fix --ext .ts,.tsx,.vue",
    "release": "vue-tsc --noEmit && vite build && electron-builder --publish onTagOrDraft",
    "patch": "npm version patch && git push origin master && git push origin --tags",
    "minor": "npm version minor && git push origin master && git push origin --tags",
    "major": "npm version major && git push origin master && git push origin --tags"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "dependencies": {
    "@electron/remote": "^2.1.2",
    "ant-design-vue": "^4.2.3",
    "electron-about": "^2.0.1",
    "electron-log": "^4.4.8",
    "electron-store": "^8.2.0",
    "fs-extra": "^11.2.0",
    "json-schema-typed": "^7.0.3",
    "regenerator-runtime": "^0.13.11",
    "sqlite3": "^5.1.7"
  },
  "devDependencies": {
    "@ant-design/icons-vue": "^7.0.1",
    "@tsconfig/node20": "^20.1.4",
    "@types/node": "^20.14.14",
    "@types/sqlite3": "^3.1.11",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-vue": "^5.1.2",
    "@vue/devtools": "^6.6.3",
    "@vue/tsconfig": "^0.5.1",
    "axios": "^1.7.3",
    "core-js": "^3.38.0",
    "electron": "^31.3.1",
    "electron-builder": "^24.13.3",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-vue": "^9.27.0",
    "highlight.js": "^11.10.0",
    "marked": "^5.1.2",
    "mitt": "^3.0.1",
    "perfect-scrollbar": "^1.5.5",
    "pinia": "^2.2.0",
    "prettier": "^3.3.3",
    "stylus": "^0.62.0",
    "tree-kill": "^1.2.2",
    "typescript": "^5.5.4",
    "unplugin-vue-components": "^0.26.0",
    "uuid": "^9.0.1",
    "vditor": "^3.10.4",
    "vite": "^5.3.5",
    "vite-plugin-electron": "^0.28.7",
    "vite-plugin-electron-renderer": "^0.14.5",
    "vite-plugin-svg-icons": "^2.0.1",
    "vue": "^3.4.35",
    "vue-router": "^4.4.2",
    "vue-tsc": "^1.8.27"
  },
  "debug": {
    "env": {
      "VITE_DEV_SERVER_URL": "http://127.0.0.1:3344/"
    }
  },
  "keywords": [
    "electron",
    "rollup",
    "markdown",
    "editor",
    "vite",
    "vue3",
    "vue"
  ]
}